{
  "$schema": "./projects/node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ischool-check-in-system": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/ischool-check-in-system",
      "sourceRoot": "projects/ischool-check-in-system/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ischool-check-in-system",
            "index": "projects/ischool-check-in-system/src/index.html",
            "main": "projects/ischool-check-in-system/src/main.ts",
            "polyfills": "projects/ischool-check-in-system/src/polyfills.ts",
            "tsConfig": "projects/ischool-check-in-system/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ischool-check-in-system/src/favicon.ico",
              "projects/ischool-check-in-system/src/assets"
            ],
            "styles": [
              "projects/ischool-check-in-system/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ischool-check-in-system/src/environments/environment.ts",
                  "with": "projects/ischool-check-in-system/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ischool-check-in-system:build:production"
            },
            "development": {
              "browserTarget": "ischool-check-in-system:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ischool-check-in-system:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ischool-check-in-system/src/test.ts",
            "polyfills": "projects/ischool-check-in-system/src/polyfills.ts",
            "tsConfig": "projects/ischool-check-in-system/tsconfig.spec.json",
            "karmaConfig": "projects/ischool-check-in-system/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ischool-check-in-system/src/favicon.ico",
              "projects/ischool-check-in-system/src/assets"
            ],
            "styles": [
              "projects/ischool-check-in-system/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
